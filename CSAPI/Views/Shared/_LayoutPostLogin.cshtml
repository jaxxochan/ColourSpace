    <title>FleetUP</title>
    <!-- Styles -->
    <!-- Bootstrap 3.3.7 -->
    <link rel="stylesheet" href="~/Content/bootstrap.min.css" />
    <link href="~/Content/bootstrap-theme.min.css" rel="stylesheet" />
    <!-- Select2 -->
    <link href="~/Content/AdminLTE/select2.min.css" rel="stylesheet" />
    <!-- Theme style -->
    <link href="~/Content/AdminLTE/AdminLTE.min.css" rel="stylesheet" />
    <!-- AdminLTE Skins. Choose a skin from the css/skins
       folder instead of downloading all of them to reduce the load. -->
    <link href="~/Content/AdminLTE/all.css" rel="stylesheet" />
    <link href="~/Content/AdminLTE/_all-skins.css" rel="stylesheet" />
    <!-- Morris chart -->
    <link href="~/Content/AdminLTE/morris.css" rel="stylesheet" />
    <!-- jvectormap -->
    <link href="~/Content/AdminLTE/jquery-jvectormap.css" rel="stylesheet" />
    <!-- Date Picker -->
    <link href="~/Scripts/bootstrap-datepicker/dist/css/bootstrap-datepicker.css" rel="stylesheet" />
    <!-- Daterange picker -->
    <link href="~/Content/AdminLTE/daterangepicker.css" rel="stylesheet" />
    <!-- Time picker -->
    <link href="~/Content/AdminLTE/bootstrap-timepicker.min.css" rel="stylesheet" />
    <!-- DataTable -->
    <link href="~/Content/AdminLTE/dataTables.bootstrap.css" rel="stylesheet" />
    <!-- DataTable Buttons-->
    <link href="~/Content/AdminLTE/buttons.dataTables.min.css" rel="stylesheet" />
    <!-- bootstrap wysihtml5 - text editor -->
    <link href="~/Content/AdminLTE/bootstrap3-wysihtml5.css" rel="stylesheet" />
    <!-- Google Font -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">
    <!-- Timeline -->
    <link href="~/Content/AdminLTE/timelinev.css" rel="stylesheet" />
    <!-- Toaster -->
    <link href="~/Content/AdminLTE/toastr.min.css" rel="stylesheet" />
    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="http://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css" />
    <!-- Custom Style -->
    <link href="~/Content/Site.css" rel="stylesheet" />
    <style>
        body {
            font-family: "Lato", sans-serif;
        }

        .sidenav {
            margin-top: 50px;
            height: 100%;
            width: 0;
            position: fixed;
            z-index: 1;
            top: 0;
            left: 0;
            background-color: #111;
            overflow-x: hidden;
            transition: 0.5s;
            padding-top: 60px;
        }

            .sidenav a {
                padding: 8px 8px 8px 32px;
                text-decoration: none;
                font-size: 25px;
                color: #818181;
                display: block;
                transition: 0.3s;
            }

                .sidenav a:hover {
                    color: #f1f1f1;
                }

            .sidenav .closebtn {
                position: absolute;
                top: 0;
                right: 25px;
                font-size: 36px;
                margin-left: 50px;
            }
    </style>
</head>
<body class="hold-transition skin-blue sidebar-mini" style="padding-top: 0px;">
    <div class="wrapper">
        @Html.Action("MenuBar", "Dashboard")
        <div class="content-wrapper">
            @RenderBody()
            <div class="row">
                <div class="col-xs-12">
                    <div id="ContentSection"></div>
                </div>
            </div>
        </div>
        <footer class="main-footer">
            <div class="pull-right hidden-xs">
                <b>Version</b> 1.0.0
            </div>
            <strong>Copyright &copy; 2020-2021 <a href="">Ninja Studio</a>.</strong> All rights
            reserved.
        </footer>
    </div>

    <!-- jQuery 3 -->
    <script src="~/Scripts/jquery/dist/jquery.min.js"></script>
    <!-- jQuery UI 1.11.4 -->
    <script src="~/Scripts/jquery-ui/jquery-ui.min.js"></script>
    <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
    <script>
        $.widget.bridge('uibutton', $.ui.button);
    </script>
    <!-- jQuery Validation 1.17.0 -->
    <script src="~/Scripts/jquery/dist/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery/dist/jquery.validate.unobtrusive.min.js"></script>
    <!-- Bootstrap 3.3.7 -->
    <script src="~/Scripts/bootstrap/dist/js/bootstrap.min.js"></script>
    <!-- Select2 -->
    @*<script src="~/Scripts/select2/dist/js/select2.full.min.js"></script>*@
    <script src="~/Scripts/select2/dist/js/select2.min.js"></script>
    <!-- toastr 2.1.3 -->
    <script src="~/Scripts/custom/toastr.min.js"></script>
    <!-- InputMask -->
    <script src="~/Scripts/input-mask/jquery.inputmask.js"></script>
    <script src="~/Scripts/input-mask/jquery.inputmask.date.extensions.js"></script>
    <script src="~/Scripts/input-mask/jquery.inputmask.extensions.js"></script>
    <!-- iCheck 1.0.1 -->
    <script src="~/Scripts/iCheck/icheck.min.js"></script>
    <!-- DataTables -->
    <script src="~/Scripts/datatables.net/js/jquery.dataTables.min.js"></script>
    <script src="~/Scripts/datatables.net-bs/js/dataTables.bootstrap.min.js"></script>
    <!-- Morris.js charts -->
    <script src="~/Scripts/raphael/raphael.min.js"></script>
    <script src="~/Scripts/morris.js/morris.min.js"></script>
    <!-- Sparkline -->
    <script src="~/Scripts/jquery-sparkline/dist/jquery.sparkline.min.js"></script>
    <!-- jvectormap -->
    <script src="~/Scripts/jvectormap/jquery-jvectormap-1.2.2.min.js"></script>
    <script src="~/Scripts/jvectormap/jquery-jvectormap-world-mill-en.js"></script>
    <!-- jQuery Knob Chart -->
    <script src="~/Scripts/jquery-knob/dist/jquery.knob.min.js"></script>
    <!-- Chart  1.0.2 -->
    <script src="~/Scripts/chart.js/Chart.js"></script>
    <!-- pdfmake 0.1.53 -->
    <script src="~/Scripts/custom/pdfmake.min.js"></script>
    <!-- daterangepicker -->
    <script src="~/Scripts/moment/min/moment.min.js"></script>
    <script src="~/Scripts/bootstrap-daterangepicker/daterangepicker.js"></script>
    <!-- datepicker -->
    <script src="~/Scripts/bootstrap-datepicker/dist/js/bootstrap-datepicker.min.js"></script>
    <!-- timepicker -->
